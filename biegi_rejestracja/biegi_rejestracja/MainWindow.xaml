<Window x:Class="biegi_rejestracja.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:biegi_rejestracja"
        mc:Ignorable="d"
        Title="RejBieg" Height="500" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="49*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="6*"></RowDefinition>
            <RowDefinition Height="49*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="1" Grid.Column="1" FontSize="28" TextAlignment="Center">Aplikacja do rejestracji na bieg</TextBlock>
        <Grid Grid.Row="2" Grid.Column="1">
            <Frame x:Name="frame" Content="Strona1" HorizontalAlignment="Center" NavigationUIVisibility="Hidden"  Width="761" Height="403" />
        </Grid>
    </Grid>
</Window>
